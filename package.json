{
  "name": "nasm-langauge-support",
  "displayName": "NASM Langauge Support",
  "description": "Language features for NASM Assembly",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nasm",
        "aliases": [
          "NASM Assembly",
          "nasm"
        ],
        "extensions": [
          ".asm",
          ".nasm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nasm",
        "scopeName": "source.nasm",
        "path": "./syntaxes/nasm.tmLanguage.json"
      }
    ]
  }
}
